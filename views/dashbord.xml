<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="act_WorkShop_sendreceived" model="ir.actions.act_window">
        <field name="name">Tickets</field>
        <field name="res_model">workshop.sendreceived</field>
            <field name="view_mode">graph</field>
              <field name="view_id" ref="AIS_WORKSHOP.WorkShop_sendreceived_view_graph"/>
        </record>



    <record model="ir.ui.view" id="Dashboard_form_view">
            <field name="name">My Dashboard</field>
            <field name="model">board.board</field>
        <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="My Dashboard">
                    <board style="1-2">
                        <column>
               <action string="Tickets"
                       name="%(AIS_WORKSHOP.act_WorkShop_sendreceived)d"/>
                        </column>

                             <column>


                        </column>
                    </board>
                </form>
            </field>
        </record>



   <record id="action_Dashboard" model="ir.actions.act_window">
        <field name="name">Dashboard</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">board.board</field>
        <field name="view_mode">form</field>
        <field name="usage">menu</field>
        <field name="view_id" ref="Dashboard_form_view"/>


    </record>
    <menuitem id="menu_Dashboard"
              name="Dashboard"
              parent="menu_workshop_root"
              action="action_Dashboard"
              sequence="1"
    />
</odoo>
